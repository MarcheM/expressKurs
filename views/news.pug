extends layout

block content
  h1(class="hero_title")= title
  form(method='get')
    input(type='text' value=search name='search')
    input(type='submit' value='Szukaj')

  each item in data   
    article 
      h1(class="news_title")=item.title 
      p(class="news_date")=item.created 
      p(class="news_desc")=item.description
