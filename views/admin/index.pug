extends ../layout

block content
  h1(class="hero_title")= title
  nav 
    ul 
      li 
        a(href="/admin/news/add") Dodaj Artukuł
  table 
    tr 
      th id 
      th tytuł
      th data dodania
      th akcja
    each item, index in data
      tr 
        td=index
        td=item.title
        td=item.created
        td
          a(href=`/admin/news/delete/${item.id}`) Usuń 
